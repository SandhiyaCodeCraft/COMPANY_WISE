def calculate_waiting_time(N, X):
   if N <= 1:...........o
    return 0
  
  completion_previous_job = (N - 1) * 10
  arrival_of_last_job = (N - 1) * X
  waiting_time = max(0,completion_previous_jobs - arrival_of_last_job)
  
  return waiting_time
LONGEST COMMON SUBSTRING:
def lcs_length(X, Y):
    m, n = len(X), len(Y)
    dp = [[0]*(n+1) for _ in range(m+1)]
    
    for i in range(1, m+1):
        for j in range(1, n+1):
            if X[i-1] == Y[j-1]:
                dp[i][j] = dp[i-1][j-1] + 1
            else:
                dp[i][j] = max(dp[i-1][j], dp[i][j-1])
    
    return dp[m][n]............accessing directly the last value

# Example
X = "AGGTAB"
Y = "GXTXAYB"
print(lcs_length(X, Y))  # Output: 4

LONGEST COMMON SUBSTRING RETURING STRING O/P:

def lcs(X, Y):
    m, n = len(X), len(Y)
    dp = [[0]*(n+1) for _ in range(m+1)]
    for i in range(1, m+1):
        for j in range(1, n+1):
            if X[i-1] == Y[j-1]:
                dp[i][j] = dp[i-1][j-1] + 1
            else:
                dp[i][j] = max(dp[i-1][j], dp[i][j-1])
    i, j = m, n
    lcs_str = []
    while i > 0 and j > 0:
        if X[i-1] == Y[j-1]:
            lcs_str.append(X[i-1])
            i -= 1
            j -= 1
        elif dp[i-1][j] > dp[i][j-1]:
            i -= 1
        else:
            j -= 1
    
    lcs_str.reverse()  
    return ''.join(lcs_str)
X = "AGGTAB"
Y = "GXTXAYB"
