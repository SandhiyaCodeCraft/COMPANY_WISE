5.
    if not text or len(text) <= 1:
        return 0

    total_distance = 0
    for i in range(1, len(text)):
        prev_pos = ord(text[i-1]) - ord('a')
        current_pos = ord(text[i]) - ord('a')
        total_distance += abs(current_pos - prev_pos)
    return total_distance
